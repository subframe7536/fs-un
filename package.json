{
  "name": "fs-un",
  "type": "module",
  "version": "0.0.1",
  "description": "unified util to manage files and directories inside a directory on different platforms",
  "author": "subframe7536",
  "license": "MIT",
  "repository": "",
  "keywords": [
    "typescript",
    "file system access"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./web": {
      "import": "./dist/web.js",
      "require": "./dist/web.cjs"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "typesVersions": {
    "*": {
      "web": [
        "./dist/web.d.ts"
      ],
      "utils": [
        "./dist/utils.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite serve dev",
    "build": "tsup",
    "release": "pnpm test && pnpm run build && bumpp --all && npm publish",
    "format": "eslint . --fix",
    "test": "vitest --run",
    "test:dev": "vitest",
    "test:asd": "esno test.ts"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "@types/wicg-file-system-access": "^2020.9.8"
  },
  "dependencies": {
    "pathe": "^1.1.2"
  },
  "devDependencies": {
    "@solid-hooks/core": "^0.1.2",
    "@subframe7536/eslint-config": "^0.5.10",
    "@subframe7536/type-utils": "^0.1.6",
    "@types/node": "^20.11.26",
    "@vitest/browser": "^1.3.1",
    "bumpp": "^9.4.0",
    "eslint": "^8.57.0",
    "esno": "^4.7.0",
    "fdir": "^6.1.1",
    "idb-keyval": "^6.2.1",
    "solid-js": "^1.8.15",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vite-plugin-solid": "^2.10.1",
    "vitest": "^1.3.1",
    "webdriverio": "^8.33.1"
  }
}
