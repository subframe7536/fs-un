{
  "name": "fs-un",
  "type": "module",
  "version": "0.4.0",
  "description": "Universal utils to manage files and directories inside a directory on different platforms",
  "author": "subframe7536",
  "license": "MIT",
  "repository": "https://github.com/subframe7536/fs-un",
  "keywords": [
    "typescript",
    "file system access"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./web": {
      "import": "./dist/web.js",
      "require": "./dist/web.cjs"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "typesVersions": {
    "*": {
      "web": [
        "./dist/web.d.ts"
      ],
      "utils": [
        "./dist/utils.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite serve dev",
    "build": "tsup",
    "release": "pnpm test && pnpm run build && bumpp --all && npm publish",
    "format": "eslint . --fix",
    "test": "vitest --run",
    "test:dev": "vitest",
    "postinstall": "pnpm exec playwright install --only-shell"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "pathe": "^1.1.2",
    "zen-mitt": "^3.0.0"
  },
  "devDependencies": {
    "@solid-hooks/core": "^0.4.1",
    "@subframe7536/eslint-config": "^1.1.1",
    "@subframe7536/type-utils": "^0.1.6",
    "@types/node": "^22.10.2",
    "@types/wicg-file-system-access": "^2023.10.5",
    "@vitest/browser": "^2.1.8",
    "bumpp": "^9.9.2",
    "eslint": "^9.17.0",
    "eslint-plugin-solid": "^0.14.5",
    "idb-keyval": "^6.2.1",
    "playwright": "^1.49.1",
    "solid-js": "^1.9.3",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vite": "^6.0.5",
    "vite-plugin-solid": "^2.11.0",
    "vitest": "^2.1.8"
  }
}
